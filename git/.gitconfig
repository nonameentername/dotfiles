[user]
	name = werner mendizabal
	email = nonameentername@gmail.com
[merge]
	tool = vimdiff
[core]
	editor = /usr/bin/vim
	excludesfile = ~/.globalignore
[alias]
	st = status
	ci = commit
	co = checkout
	vimdiff = difftool -y -t vimdiff
	tree = log --graph --decorate --pretty=oneline --abbrev-commit
[color]
	branch = auto
	interactive = auto
	status = auto
	diff = auto
[diff]
	tool = vimdiff
[push]
	default = simple
[gitreview]
	username = nonameentername
[remote-hg]
	hg-git-compat = true
[hub]
	protocol = https
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[difftool]
    trustExitCode = true
